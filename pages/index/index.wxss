/* 我的 */
.mask {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 99999;
  transition: all 0.01s 0.28s;
  -ms-transition: all 0.01s 0.28s;
  -moz-transition: all 0.01s 0.28s;
  -webkit-transition: all 0.01s 0.28s;
  -o-transition: all 0.01s 0.28s;
  transform: translateX(-750rpx);
  -ms-transform: translateX(-750rpx);
  -moz-transform: translateX(-750rpx);
  -webkit-transform: translateX(-750rpx);
  -o-transform: translateX(-750rpx);
}
.masked {
  transition: all 0.01s;
  -ms-transition: all 0.01s;
  -moz-transition: all 0.01s;
  -webkit-transition: all 0.01s;
  -o-transition: all 0.01s;
  transform: translateX(0rpx);
  -ms-transform: translateX(0rpx);
  -moz-transform: translateX(0rpx);
  -webkit-transform: translateX(0rpx);
  -o-transform: translateX(0rpx);
}
.slider {
  width: 546rpx;
  height: 100%;
  background: #fff;
  transition: all 0.5s;
  -ms-transition: all 0.5s;
  -moz-transition: all 0.5s;
  -webkit-transition: all 0.5s;
  -o-transition: all 0.5s;
  transform: translateX(-546rpx);
  -ms-transform: translateX(-546rpx);
  -moz-transform: translateX(-546rpx);
  -webkit-transform: translateX(-546rpx);
  -o-transform: translateX(-546rpx);
}

.slidered {
  transition: all 0.5s;
  -ms-transition: all 0.5s;
  -moz-transition: all 0.5s;
  -webkit-transition: all 0.5s;
  -o-transition: all 0.5s;
  transform: translateX(0rpx);
  -ms-transform: translateX(0rpx);
  -moz-transform: translateX(0rpx);
  -webkit-transform: translateX(0rpx);
  -o-transform: translateX(0rpx);
}

